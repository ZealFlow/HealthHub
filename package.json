{
  "name": "healsync",
  "version": "1.0.0",
  "description": "HealSync",
  "main": "server.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typeorm:run": "npm run typeorm migration:run",
    "typeorm": "ts-node --require tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZealFlow/HealSync.git"
  },
  "keywords": [
    "HealSync",
    "edoctor",
    "HospitalOnline",
    "Medicine"
  ],
  "author": "TinNguyen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ZealFlow/HealSync/issues"
  },
  "homepage": "https://github.com/ZealFlow/HealSync#readme",
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "inversify": "^6.0.2",
    "inversify-express-utils": "^6.4.6",
    "mysql2": "^3.7.1",
    "nodemon": "^3.0.3",
    "reflect-metadata": "^0.2.1",
    "typeorm-faker": "^1.3.12"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.3.3"
  }
}
